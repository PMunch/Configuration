!URxvt.font: 9x15
!URxvt.font: xft:Inconsolata for Powerline:size=14,xft:FontAwesome,xft:Icons,xft:DejaVu Sans Mono:style=Book
URxvt.font: xft:InconsolataGo Nerd Font:style=Regular:size=15,xft:Icons,xft:DejaVu Sans:style=Book
Xft.dpi: 96
Xft.antialias: true
Xft.hinting: true
Xft.rgba: rgb
Xft.autohint: false
Xft.hintstyle: hintslight
Xft.lcdfilter: lcddefault
!Xcursor.theme: Chameleon-Pearl-Regular-0.5
Xcursor.theme: DMZ-white
Xcursor.size:  16
URxvt.depth: 32
URxvt.background: [62]#282828
! URxvt.background: [62]#000000
URxvt*scrollBar:                      false
URxvt*mouseWheelScrollPage:           false
URxvt*cursorBlink:                    true
URxvt*foreground:                     #ebdbb2
URxvt*saveLines:                      5000
URxvt.keysym.C-Page_Up:               \033[5;5~
URxvt.keysym.C-Page_Down:             \033[6;5~

URxvt*urgentOnBell: true
URxvt.perl-ext-common: default,keyboard-select,selection-autotransform
URxvt.keysym.C-Escape: perl:keyboard-select:activate
URxvt.selection-autotransform.0: s/^([^:[:space:]]+):(\\d+):(\\d+): (.+)/vim +$2 +'norm $3|' +'redraw|echom "$4"' \\Q$1\\E\\x0d/
URxvt.selection-autotransform.1: s/^([^:[:space:]]+):(\\d+):(\\d+):?/vim +$2 +'norm $3|'\\Q$1\\E\\x0d/
URxvt.selection-autotransform.2: s/^([^([:space:]]+)\\((\\d+), (\\d+)\\) (.+)/vim +$2 +'norm $3|' +'redraw|echom "$4"' \\Q$1\\E\\x0d/
URxvt.selection-autotransform.3: s/^([^([:space:]]+)\\((\\d+), (\\d+)\\)/vim +$2 +'norm $3|' \\Q$1\\E\\x0d/
! Copy pasting
!URxvt.perl-ext-common: default,clipboard
!URxvt.iso14755: False
!URxvt.keysym.Shift-Control-C: perl:clipboard:copy
!URxvt.keysym.Shift-Control-V: perl:clipboard:paste
!URxvt.clipboard.copycmd:  xsel -ib
!URxvt.clipboard.pastecmd: xsel -ob

! ROFI config
rofi.color-enabled: true
rofi.color-window: argb:99323232, #000000, #000000
rofi.color-normal: #323232, #86A4DB, #323232, #86A4DB, #323232
rofi.color-active: #323232, #86A4DB, #323232, #86A4DB, #323232
rofi.color-urgent: #323232, #900000, #323232, #900000, #fdf6e3
rofi.font: System Raleway UltraLight 18
rofi.separator-style: none
rofi.bw: 0
rofi.fake-transparency: true
rofi.eh: 1
rofi.hide-scrollbar: true
rofi.line-margin: 25


